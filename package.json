{
  "name": "gen-interface-plugin",
  "displayName": "gen-interface-plugin",
  "description": "",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.83.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "menus": {
      "explorer/context": [
        {
          "command": "gen-interface.genInterface",
          "group": "2_workspace",
          "when": "resourceFilename =~ /\\.ts$/ && !(resourceFilename =~ /\\.type\\.ts$/)"
        },
        {
          "command": "gen-interface.genInterfaceForce",
          "group": "2_workspace",
          "when": "resourceFilename =~ /\\.ts$/ && !(resourceFilename =~ /\\.type\\.ts$/)"
        }
      ]
    },
    "commands": [
      {
        "command": "gen-interface.genInterface",
        "title": "生成interface类型"
      },
      {
        "command": "gen-interface.genInterfaceForce",
        "title": "生成interface类型(合并)"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.83.0",
    "@types/mocha": "^10.0.2",
    "@types/node": "18.x",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "eslint": "^8.50.0",
    "glob": "^10.3.3",
    "mocha": "^10.2.0",
    "typescript": "^5.2.2",
    "@vscode/test-electron": "^2.3.4"
  }
}